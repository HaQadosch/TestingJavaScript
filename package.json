{
  "name": "testingjavascript",
  "version": "1.0.0",
  "description": "https://testingjavascript.com",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "format": "npm run prettier -- --write",
    "jest": "jest",
    "precommit": "npm run validate",
    "pretest": "rollup -c",
    "prettier": "prettier \"**/*.+(js|jsx|css|less|scss|ts|tsx|graphql)\"",
    "test": "node --require ./setup_globals.js bundle.js",
    "validate": "npm run format && npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaQadosch/TestingJavaScript.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HaQadosch/TestingJavaScript/issues"
  },
  "homepage": "https://github.com/HaQadosch/TestingJavaScript#readme",
  "dependencies": {
    "prettier": "^1.17.0",
    "rollup": "^1.10.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "flow-bin": "^0.97.0",
    "husky": "^2.1.0",
    "jest": "^24.7.1"
  }
}